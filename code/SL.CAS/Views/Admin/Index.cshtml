        //获取url参数方法001
        function getQueryString(name) {
            var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
            var r = window.location.search.substr(1).match(reg);
            if (r != null) return unescape(r[2]); return null;
        }


        //刷新当前的选项卡
        function refreshCurrTab() {
            var curIndex = -1;
            var curUrl = "";
            $("#page-wrapper .page-tabs-content a").each(function (i, item) {
                if ($(this).hasClass("active")) {
                    curIndex = i;
                    return;
                }
            });

            if (curIndex != -1) {
                var dom = $("#page-wrapper #content-main iframe:eq(" + (curIndex - 1) + ")");
                dom.attr("src", dom.attr("src"));
            }
        }


    </script>
